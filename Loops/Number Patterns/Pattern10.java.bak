class Pattern10 
{
	public static void main(String[] args) 
	{
		int n=15;
		int a=1;
		int b=1;
		int step = 9;
		int c=0;
		
		for (int i=1;i<=n ;i+=a )//i1
		{
			//int step =4;
			int var=0;
			int temp = a;//1
			System.out.print(a+" ");//1
			temp+=step;//1+9=10
			for (int j=a;j<a+a-1 ;j++ )//1<1+1-1=1
			{
				//temp+=step;//4+3
				System.out.print(temp+var+" ");//7+2=9
				var+=c;//2
				//step--;
			}
			System.out.println();
			c+=2;
			a++;//a4
			step-=2;//3
		}
	}
}
