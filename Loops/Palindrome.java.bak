import java.util.Scanner;

class Palindrome
{
	public static void main(String[] args) 
	{
		Scanner sc = new Scanner(System.in);
		System.out.println("Enter a word: ");
		String word = sc.next();

		String reverse = null; 

		for (int i = word.length()-1; i >= 0; i--)
		{
			reverse = word.charAt(i);
		}

		System.out.print(reverse);
	}
}
