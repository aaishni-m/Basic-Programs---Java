class K
{
	public static void main(String[] args) 
	{
		int n = 20;
		for (int i=0;i<n ;i++ )
		{
			for (int j=0;j<n ;j++ )
			{
				if (j==n/4&&i>=n/4&&i<=3*n/4||j==n/2&&i==n/4||i+j==15&&i>=n/4&&i<=n/2||i==3*n/4&&j==n/2||i-j==5&&i>=n/2)
				{
					System.out.print("*"+" ");
				}
				else{
					System.out.print(" "+" ");
				}
			}
			System.out.println();
		}
	}
}

