class IntConversion 
{
	public static void main(String[] args) 
	{
		int i = 122;
		byte b = (byte) i;
		char c = (char) i;
		short s = (int) i;
		long l = (long)i;
		float f = (float)i;
		double d = (double) i;

		System.out.println(b);
		System.out.println(c);
		System.out.println(s);
		System.out.println(l);
		System.out.println(f);
		System.out.println(d);
	}
}
